{
  "name": "da1/da1_n5",
  "description": "Shop bán quần áo",
  "type": "project",
  "require": {
    "php": "^7.4 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/bootstrap.php"
    ]
  },
  "scripts": {
    "start": "php -S localhost:8000 -t public"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
